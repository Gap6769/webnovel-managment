{
  "expo": {
    "name": "WebNovel Manager",
    "slug": "webnovel-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-square.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2a2a2a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webnovelmanager.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-square.png",
        "backgroundColor": "#2a2a2a"
      },
      "package": "com.webnovelmanager.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": [
      "assets/fonts/gothampro.ttf",
      "assets/fonts/gothampro_black.ttf",
      "assets/fonts/gothampro_blackitalic.ttf",
      "assets/fonts/gothampro_bold.ttf",
      "assets/fonts/gothampro_bolditalic.ttf",
      "assets/fonts/gothampro_italic.ttf",
      "assets/fonts/gothampro_light.ttf",
      "assets/fonts/gothampro_lightitalic.ttf",
      "assets/fonts/gothampro_medium.ttf",
      "assets/fonts/gothampro_mediumitalic.ttf"
    ],
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "386ae90d-ad80-4e8c-b33f-66673484f08b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/386ae90d-ad80-4e8c-b33f-66673484f08b"
    }
  }
}
